{
  "venvPath": "integration-tests",
  "venv": ".venv",
  "typeCheckingMode": "basic",
  "include": [
    "integration-tests/pkg",
    "integration-tests/testdata/programs/hcloud-ha-py"
  ],
  "executionEnvironments": [
    {
      "root": "integration-tests/pkg",
      "extraPaths": [
        "integration-tests/pkg"
      ]
    },
    {
      "root": "integration-tests/testdata/programs/hcloud-ha-py",
      "extraPaths": [
        "integration-tests/pkg"
      ],
      "reportMissingImports": "warning"
    }
  ]
}
