{
  "compilerOptions": {
    "strict": true,
    "outDir": "bin",
    "target": "es2024",
    "module": "Node16",
    "moduleResolution": "Node16",
    "sourceMap": false,
    "experimentalDecorators": true,
    "pretty": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitReturns": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true
  },
  "ts-node": {
    "logError": true
  },
  "files": ["index.ts"]
}
